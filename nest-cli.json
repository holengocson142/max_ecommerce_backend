{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ecommerce-gateway",
  "monorepo": true,
  "root": "apps/ecommerce-gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/ecommerce-gateway/tsconfig.app.json"
  },
  "projects": {
    "ecommerce-gateway": {
      "type": "application",
      "root": "apps/ecommerce-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/ecommerce-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ecommerce-gateway/tsconfig.app.json"
      }
    },
    "profile": {
      "type": "application",
      "root": "apps/profile",
      "entryFile": "main",
      "sourceRoot": "apps/profile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profile/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "caching": {
      "type": "library",
      "root": "libs/caching",
      "entryFile": "index",
      "sourceRoot": "libs/caching/src",
      "compilerOptions": {
        "tsConfigPath": "libs/caching/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}